{
  "name": "oh-my-mock",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:server": "node ./test-site/server.js",
    "build": "ng build --output-hashing none --base-href=index.html && yarn build:content && yarn build:injected && yarn build:background",
    "build:both": "yarn build:content && yarn build:injected && yarn build:background",
    "build:content": "tsc --build ./tsconfig-content.json && browserify --node dist/content/index.js > dist/content.js && rm -rf dist/content && cp manifest.json dist/",
    "build:injected": "tsc --build ./tsconfig-inject.json && browserify --node ./dist/injected/index.js > ./dist/injected.js && rm -rf dist/injected dist/shared",
    "build:background": "tsc --build ./tsconfig-background.json && browserify --node ./dist/background > ./dist/bg.js && mv dist/bg.js dist/background.js",
    "build:watchng": "ng build  --base-href=index.html --watch",
    "build:custom": "node ./scripts/monitor.js",
    "build:watch": "run-p build:custom build:watchng",
    "zip": "cd dist && zip -r ../oh-my-mock.zip . && cd ..",
    "test": "npx jest",
    "lint": "tsc --project tsconfig.json --noEmit && eslint . --ext js,ts,json --quiet --fix",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.5",
    "@angular/cdk": "^11.1.1",
    "@angular/common": "~11.0.5",
    "@angular/compiler": "~11.0.5",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~11.0.5",
    "@angular/material": "^11.1.1",
    "@angular/platform-browser": "~11.0.5",
    "@angular/platform-browser-dynamic": "~11.0.5",
    "@angular/router": "~11.0.5",
    "@materia-ui/ngx-monaco-editor": "^5.0.0",
    "@ngneat/hot-toast": "^1.0.11",
    "@ngneat/overview": "^1.0.0",
    "@ngxs-labs/dispatch-decorator": "^2.1.0",
    "@ngxs/store": "^3.7.1",
    "@types/typescript": "^2.0.0",
    "body-parser": "^1.19.0",
    "chokidar": "^3.5.1",
    "content-type-parser": "^1.0.2",
    "express": "^4.17.1",
    "highlight.js": "^10.5.0",
    "mime-types": "^2.1.28",
    "monaco-editor": "^0.22.3",
    "node-sass": "^5.0.0",
    "rxjs": "~6.6.0",
    "sass": "^1.32.6",
    "scss-reset": "^1.1.10",
    "tslib": "^2.0.0",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^11.0.0",
    "@angular-devkit/build-angular": "~0.1100.5",
    "@angular/cli": "~11.0.5",
    "@angular/compiler-cli": "~11.0.5",
    "@ng-apimock/core": "^2.5.0",
    "@types/chrome": "^0.0.127",
    "@types/jasmine": "~3.6.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "browserify": "^17.0.0",
    "codelyzer": "^6.0.0",
    "eslint": "^7.21.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jest": "^26.6.3",
    "jest-preset-angular": "^8.4.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
