{
  "name": "oh-my-mock",
  "version": "2.9.6",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:server": "node ./test-site/server.js",
    "build": "ng build --output-hashing none --base-href=index.html && yarn build:other && node ./scripts/token-replace.js",
    "build:other": "yarn build:content && yarn build:injected && yarn build:background && yarn build:splash",
    "build:content": "tsc --build ./tsconfig-content.json && browserify --node dist/content/index.js > dist/content.js && rm -rf dist/content && cp manifest.json dist/",
    "build:injected": "tsc --build ./tsconfig-inject.json && browserify --node ./dist/injected/index.js > ./dist/injected.js && rm -rf dist/injected",
    "build:background": "tsc --build ./tsconfig-background.json && browserify --node ./dist/background > ./dist/bg.js && mv dist/bg.js dist/background.js",
    "build:watchng": "ng build  --base-href=index.html --watch",
    "build:custom": "node ./scripts/monitor.js",
    "build:watch": "run-p build:custom build:watchng",
    "build:splash": "cp src/splash-screen.html ./dist",
    "zip": "rm -rf dist && yarn build && cd dist && zip -r ../oh-my-mock.zip . && cd ..",
    "test": "npx jest",
    "test:stryker": "stryker run",
    "lint": "tsc --project tsconfig.json --noEmit && eslint . --ext js,ts,json --quiet --fix",
    "e2e": "ng e2e",
    "chrome:crx": "\"/Applications/Google Chrome.app/Contents/MacOS/Google Chrome\" --pack-extension=./dist",
    "crx": "./scripts/build-crx.sh ./dist ./scripts/dist.pem"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.1.0",
    "@angular/cdk": "^11.1.1",
    "@angular/common": "~12.1.0",
    "@angular/compiler": "~12.1.0",
    "@angular/core": "~12.1.0",
    "@angular/forms": "~12.1.0",
    "@angular/material": "^11.1.1",
    "@angular/platform-browser": "~12.1.0",
    "@angular/platform-browser-dynamic": "~12.1.0",
    "@angular/router": "~12.1.0",
    "@materia-ui/ngx-monaco-editor": "^5.1.0",
    "@ngneat/hot-toast": "^3.1.0",
    "@ngneat/overview": "^2.0.2",
    "@ngneat/until-destroy": "^8.0.4",
    "@ngxs-labs/dispatch-decorator": "^3.1.0",
    "@ngxs/store": "^3.7.2",
    "@types/typescript": "^2.0.0",
    "body-parser": "^1.19.0",
    "chokidar": "^3.5.1",
    "compare-versions": "^3.6.0",
    "content-type-parser": "^1.0.2",
    "express": "^4.17.1",
    "faker": "^5.5.3",
    "jsonpath-plus": "^5.1.0",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.31",
    "monaco-editor": "^0.25.2",
    "rxjs": "~6.6.0",
    "sass": "^1.32.6",
    "scss-reset": "^1.1.10",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^11.0.0",
    "@angular-devkit/build-angular": "~12.1.0",
    "@angular/cli": "~12.1.0",
    "@angular/compiler-cli": "~12.1.0",
    "@ng-apimock/core": "^3.0.1",
    "@stryker-mutator/core": "^5.0.1",
    "@stryker-mutator/jest-runner": "^5.0.1",
    "@stryker-mutator/karma-runner": "^5.0.1",
    "@stryker-mutator/typescript-checker": "^5.0.1",
    "@types/chrome": "^0.0.127",
    "@types/jest": "^26.0.20",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "browserify": "^17.0.0",
    "codelyzer": "^6.0.0",
    "eslint": "^7.21.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "jest-environment-jsdom-sixteen": "^2.0.0",
    "jest-preset-angular": "^8.4.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "stryker-cli": "^1.0.1",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.4"
  }
}
